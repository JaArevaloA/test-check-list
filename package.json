{
  "name": "plantilla-angular-v13",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "start:qa": "ng serve -c qa --port 4201",
    "start:prod": "ng serve --prod --port 4201",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "compila-dev": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=dev --output-path=dist",
    "compila-qa": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=qa --output-path=dist",
    "compila-prod": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=production --output-path=dist",
    "all": "npm run dev && npm run qa && npm run prod"
  },
  "private": true,
  "dependencies": {
